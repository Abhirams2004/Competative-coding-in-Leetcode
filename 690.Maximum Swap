class Solution(object):
    def maximumSwap(self, num):
        num = list(str(num))
        for i in range(len(num)-1):
            maxi = max(num[i+1:])
            ind = num[i+1:].index(maxi)
            if maxi>num[i]:
                ind = len(num) - 1 - num[::-1].index(maxi)
                num[i], num[ind] = num[ind], num[i]
                break
        return int(''.join(num))
